{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/trello-v2-backend/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": ["../protos/*.proto"],
    "watchAssets": true,
    "webpack": false,
    "tsConfigPath": "apps/trello-v2-backend/tsconfig.app.json"
  },
  "projects": {
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "trello-v2-backend": {
      "type": "application",
      "root": "apps/trello-v2-backend",
      "entryFile": "main",
      "sourceRoot": "apps/trello-v2-backend/src",
      "compilerOptions": {
        "tsConfigPath": "apps/trello-v2-backend/tsconfig.app.json"
      }
    },
    "cardlist": {
      "type": "application",
      "root": "apps/cardlist",
      "entryFile": "main",
      "sourceRoot": "apps/cardlist/src",
      "compilerOptions": {
        "tsConfigPath": "apps/cardlist/tsconfig.app.json"
      }
    },
    "board": {
      "type": "application",
      "root": "apps/board",
      "entryFile": "main",
      "sourceRoot": "apps/board/src",
      "compilerOptions": {
        "tsConfigPath": "apps/board/tsconfig.app.json"
      }
    },
    "workspace": {
      "type": "application",
      "root": "apps/workspace",
      "entryFile": "main",
      "sourceRoot": "apps/workspace/src",
      "compilerOptions": {
        "tsConfigPath": "apps/workspace/tsconfig.app.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/trello-v2-backend"
}
